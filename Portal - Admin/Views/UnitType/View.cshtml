@model ResComm.Web.Models.UnitType
@{
    ViewBag.Title = "Street/Block Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Title { Details }
@section ContentHeader{
    <script>
            $(function () {
                $('#main_title').html("Street/Block");
            });
    </script>

    <div class="pull-right">
        <a href="@Url.Action("List", "UnitType")" class="btn btn-primary inline"><i class="fa fa-arrow-left"></i> Back</a>
        <a href="@Url.Action("Edit", "UnitType", new { Id = Model.UnitTypeId })" class="btn btn-primary inline"><i class="fa fa-edit"></i> Edit</a>
    </div>
}

<div class="row">
    <div class="col-md-6">
        <div class="box box-primary">
            <div class="box-header">
                <h3 class="box-title">Detail Information</h3>
            </div><!-- /.box-header -->
            <div class="box-body">
                <div class="form-group">
                    @Html.LabelFor(m => m.CategoryName)
                    <div>
                        @Model.CategoryName
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.CategoryValue)
                    <div>
                        @Model.CategoryValue
                    </div>
                </div>
                @*@if (Model.CategoryName == UNIT_TYPE_CATEGORY.Block.ToString())
                { 
                    <div class="form-group">
                        @Html.LabelFor(m => m.Floor)
                        <div>
                            @Model.Floor
                        </div>
                    </div>
                }*@
            </div><!-- /.box-body -->
        </div><!-- /.box -->

    </div><!--/.col (left) -->
</div><!-- /.row -->
