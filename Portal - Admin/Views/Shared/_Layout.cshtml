<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html>
<head>
    <meta charset="UTF-8">
    <title>@ViewBag.Title</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
    <!-- Bootstrap 3.3.6 css -->
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <!-- Font Awesome Icons -->
    <link href="@Url.Content("~/Content/font-awesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <!-- Ionicons -->
    <link href="@Url.Content("~/Content/ionicons/css/ionicons.min.css")" rel="stylesheet" type="text/css" />
    <!-- iCheck for checkboxes and radio inputs -->
    <link href="@Url.Content("~/Content/plugins/iCheck/all.css")" rel="stylesheet" type="text/css" />
    <!-- Theme style -->
    <link href="@Url.Content("~/Content/dist/css/AdminLTE.min.css")" rel="stylesheet" type="text/css" />
    <!-- AdminLTE Skins. We have chosen the skin-blue for this starter
          page. However, you can choose any other skin. Make sure you
          apply the skin class to the body tag so the changes take effect.
    -->
    <link href="@Url.Content("~/Content/dist/css/skins/_all-skins.min.css")" rel="stylesheet" type="text/css" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

    <!-- Toastr -->
    <link rel="stylesheet" href="@Url.Content("~/Content/toastr/toastr.css")">

    <link rel="stylesheet" href="@Url.Content("~/Content/main.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/default.css")">


    <!-- REQUIRED JS SCRIPTS -->
    <!-- jQuery 2.1.3 -->
    <script src="@Url.Content("~/Content/plugins/jQuery/jQuery-2.1.3.min.js")"></script>
    <!-- Bootstrap 3.3.2 JS -->
    <script src="@Url.Content("~/Content/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <!-- jQuery UI 1.10.3 -->
    <script src="@Url.Content("~/Content/plugins/jQueryUI/jquery-ui.1.11.2.min.js")"></script>
    <!-- AdminLTE App -->
    <script src="@Url.Content("~/Content/dist/js/app.min.js")" type="text/javascript"></script>
    <!-- iCheck 1.0.1 -->
    <script src="@Url.Content("~/Content/plugins/iCheck/icheck.min.js")" type="text/javascript"></script>
    <script type="text/javascript">
        //iCheck for checkbox and radio inputs
        $(function () {
            $('input[type="checkbox"].minimal, input[type="radio"].minimal').iCheck({
                checkboxClass: 'icheckbox_minimal-blue',
                radioClass: 'iradio_minimal-blue'
            });
        });
    </script>
    <!-- SlimScroll -->
    <script src="@Url.Content("~/Content/plugins/slimScroll/jquery.slimscroll.min.js")" type="text/javascript"></script>
    <!-- FastClick -->
    <script src='@Url.Content("~/Content/plugins/fastclick/fastclick.min.js")'></script>

    <!-- Toastr -->
    <script src="@Url.Content("~/Content/toastr/toastr.js")"></script>
    <script type="text/javascript">
        //alert override
        window.alert = function (msg, type) {
            if (type == "success") {
                toastr.success(msg, "Success");
            }
            else if (type == "warning") {
                toastr.warning(msg, "Warning");
            }
            else if (type == "error") {
                toastr.error(msg, "Error");
            }
            else {
                toastr.info(msg, "Info");
            }
        }
    </script>

    <!-- Jquery Validation -->
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript">
        //tooltip for validation
        $(function () {
            $(document).tooltip({
                items: ".input-validation-error",
                content: function () {
                    var validationtxt = "";
                    if ($(this).val() == "") {
                        validationtxt = $(this).attr('data-val-required');
                    }
                    else {
                        if ($(this).attr('data-val-length') != undefined) {
                            validationtxt = $(this).attr('data-val-length')
                        }
                        else if ($(this).attr('data-val-equalto') != undefined) {
                            validationtxt = $(this).attr('data-val-equalto')
                        }
                        else if ($(this).attr('data-val-regex') != undefined) {
                            validationtxt = $(this).attr('data-val-regex')
                        }
                    }
                    return validationtxt;
                },
                position: {
                    my: "center bottom-8",
                    at: "center top",
                    using: function (position, feedback) {
                        $(this).css(position);
                        $("<div>")
                            .addClass("arrow")
                            .addClass(feedback.vertical)
                            .addClass(feedback.horizontal)
                            .appendTo(this);
                    }
                }
            });
        });
    </script>
    
    <!-- FancyBox -->
    <link rel="stylesheet" href="@Url.Content("~/Content/fancybox/jquery.fancybox.css")">
    <script src='@Url.Content("~/Content/fancybox/jquery.fancybox.js")'></script>
    <script src='@Url.Content("~/Content/fancybox/jquery.mousewheel-3.0.6.pack.js")'></script>
    <script type="text/javascript">
        $('.fancybox').fancybox({
            afterShow: function () {
                var iviewer = $('.fancybox-inner').iviewer(
                {
                    src: $.fancybox.current.href,
                });
                $('img.fancybox-image').attr('src', '');
            }
        });
    </script>

    <!-- Prevent Page Loading from Cache On Back Button Clicked -->
    <script>
        function reload_on_back_btn(){
            if (!!window.performance && window.performance.navigation.type == 2) {
                window.location.reload();
            }
        }
    </script>
</head>

@{
    string ControllerName = HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString();
    string ActionName = ViewContext.RouteData.Values["action"].ToString();
}
<body class="skin-blue">
    @if (TempData["Message"] != null)
    {
        <script type="text/javascript">
            alert("@Html.Raw(TempData["Message"])");
        </script>
    }
    <div class="wrapper">
        <!-- Main Header -->
        <header class="main-header">
            <!-- Logo -->
            <a href="@Url.Action("Index","Home")" class="logo"><img src="~/Content/Logo - Horizontal (Small).png" /></a>
            <!-- Header Navbar -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- User Account Menu -->
                        <li class="dropdown user user-menu">
                            <!-- Menu Toggle Button -->
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <!-- The user image in the navbar-->
                                <img src="@Url.Content("~/Content/dist/img/user.png")" class="user-image" alt="User Image" />
                                <!-- hidden-xs hides the username on small devices so only the image appears. -->
                                <span class="hidden-xs">@(Session["Name"] ?? "")</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- The user image in the menu -->
                                <li class="user-header" style="height: auto;">
                                    <img src="@Url.Content("~/Content/dist/img/user.png")" class="img-circle" alt="User Image" />
                                    <p>
                                        @(Session["Name"] ?? "")
                                        <small>@(Session["Email"] ?? "")</small>
                                    </p>
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-right">
                                        <a href="@Url.Action("LogOut", "Home")" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header">MAIN NAVIGATION</li>
                    <li @if (ControllerName.ToLower() == "home" && ActionName.ToLower() == "index") { @Html.Raw("class='active'")}>
                        <a href="@Url.Action("Index","Home")">
                            <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                        </a>
                    </li>
                    <li class="treeview @if (ControllerName.ToLower() == "myaccount") { @Html.Raw("active") }">
                        <a href="#">
                            <i class="fa fa-user"></i> <span>My Account</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("MySubscription","MyAccount")"><i class="fa fa-circle-o"></i> My Subscription</a></li>
                            <li><a href="@Url.Action("MyInvoices", "MyAccount")"><i class="fa fa-circle-o"></i> My Invoices</a></li>
                        </ul>
                    </li>
                    <li class="treeview @if (ControllerName.ToLower() == "property" || ControllerName.ToLower() == "unit" || ControllerName.ToLower() == "unittype") { @Html.Raw("active") }">
                        <a href="#">
                            <i class="fa fa-home"></i> <span>Property Setup</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Details","Property")"><i class="fa fa-circle-o"></i> Property Details</a></li>
                            <li><a href="@Url.Action("List", "UnitType")"><i class="fa fa-circle-o"></i> Street/Block Setup</a></li>
                            <li><a href="@Url.Action("List", "Unit")"><i class="fa fa-circle-o"></i> Unit Setup</a></li>
                        </ul>
                    </li>
                    <li @if (ControllerName.ToLower() == "member" && ActionName.ToLower().Contains("owner")) { @Html.Raw("class='active'") }>
                        <a href="@Url.Action("OwnerList", "Member")">
                            <i class="fa fa-key"></i> <span>Owner</span>
                        </a>
                    </li>
                    <li @if (ControllerName.ToLower() == "member" && ActionName.ToLower().Contains("resident")) { @Html.Raw("class='active'")  }>
                        <a href="@Url.Action("ResidentList", "Member")">
                            <i class="fa fa-users"></i> <span>Resident</span>
                        </a>
                    </li>
                    <li @if (ControllerName.ToLower() == "member" && ActionName.ToLower().Contains("inactivemember")) { @Html.Raw("class='active'")   }>
                        <a href="@Url.Action("InactiveMemberList", "Member")">
                            <i class="fa fa-user-times"></i> <span>Unoccupied Member</span>
                        </a>
                    </li>
                    <li @if (ControllerName.ToLower() == "announcement") { @Html.Raw("class='active'")      }>
                        <a href="@Url.Action("List", "Announcement")">
                            <i class="fa fa-list-alt"></i> <span>Announcement</span>
                        </a>
                    </li>
                    <li @if (ControllerName.ToLower() == "documents") { @Html.Raw("class='active'")    }>
                        <a href="@Url.Action("List", "Documents")">
                            <i class="fa fa-pencil"></i> <span>Document</span>
                        </a>
                    </li>
                    <li @if (ControllerName.ToLower() == "ticket") { @Html.Raw("class='active'")    }>
                        <a href="@Url.Action("List", "Ticket")">
                            <i class="fa fa-comments-o"></i> <span>Complain & Defect</span>
                        </a>
                    </li>
                    <li @if (ControllerName.ToLower() == "facility") { @Html.Raw("class='active'")      }>
                        <a href="@Url.Action("List", "Facility")">
                            <i class="fa fa-book"></i> <span>Facility Booking</span>
                        </a>
                    </li>
                    <li @if (ControllerName.ToLower() == "billing") { @Html.Raw("class='active'")       }>
                        <a href="@Url.Action("List", "Billing")">
                            <i class="fa fa-money"></i> <span>Online Bill Payment</span>
                        </a>
                    </li>
                    <li class="treeview @if (ControllerName.ToLower() == "setting" || ControllerName.ToLower() == "ticketcategory") { @Html.Raw("active") }">
                        <a href="#">
                            <i class="fa fa-cogs"></i> <span>Settings</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Admin","Setting")"><i class="fa fa-circle-o"></i> Admin Settings</a></li>
                            <li><a href="@Url.Action("List","TicketCategory")"><i class="fa fa-circle-o"></i> Ticket Category Settings</a></li>
                        </ul>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    <span id="main_title">@ControllerName</span>
                    <small>@RenderSection("Title", false)</small>
                    <span class="w200">&nbsp;</span>
                    @RenderSection("ContentHeader", false)
                </h1>

                @*<ol class="breadcrumb">
                    <li><a href="@Url.Action("Home","Index")"><i class="fa fa-dashboard"></i> Dashboard</a></li>
                    @RenderSection("Breadcrumb", false)
                </ol>*@
            </section>
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
            <div class="pull-right hidden-xs">
                <!-- Anything you want -->
            </div>
            <!-- Default to the left -->
            <strong>Copyright &copy; 2016 <!-- <a href="#">Company</a>. --></strong> All rights reserved.
        </footer>
    </div><!-- ./wrapper -->

    <!-- Image Orientation -->
    <script type="text/javascript">
        $('.img-responsive-flex').each(function (i, obj) {
            if ($(obj).height() < $(obj).width()) {
                $(obj).addClass("img-responsive-flex-landscape");
            }
            else {
                $(obj).addClass("img-responsive-flex-portrait");
            }
        });
    </script>

    <!-- LabelFor Asterisk for Required field -->
    <script type="text/javascript">
        $('input').each(function () {
            var req = $(this).attr('data-val-required');
            if (undefined != req) {
                var label = $('label[for="' + $(this).attr('id') + '"]');
                var text = label.text();
                if (text.length > 0) {
                    label.append('<span style="color:red"> *</span>');
                }
            }
        });
        $('select').each(function () {
            var req = $(this).attr('data-val-required');
            if (undefined != req) {
                var label = $('label[for="' + $(this).attr('id') + '"]');
                var text = label.text();
                if (text.length > 0) {
                    label.append('<span style="color:red"> *</span>');
                }
            }
        });
    </script>

    <!-- Prevent Double Submission -->
    <script type="text/javascript">
        $("form").submit(function () {
            var custom_validation = 'true';
            if (typeof form_submit_custom_check == 'function') {
                custom_validation = form_submit_custom_check();
            }

            if (custom_validation == 'true') {
                if ($(this).valid()) {
                    var form = $(this);
                    form.find("input[type=submit]").attr("disabled", "disabled")
                    form.find("input[type=submit]").attr("value", "Processing...");
                    // prevent the user from hitting ENTER on a field to submit again
                    form.find("input").attr("readonly", true);
                    document.body.style.cursor = "wait";
                }
            }
            else {
                return false;
            }
        });
    </script>
</body>
</html>