@model ResComm.Web.Models.AdminSetup
@{
    Layout = null;
}

@Html.HiddenFor(m => m.PropertyInfo.PropertyId)

<div class="col-md-3">
</div>

<!-- left column -->
<div class="col-md-6">
    <div class="box box-danger">
        <div class="box-header">
            <h3 class="box-title">Property Information</h3>
        </div><!-- /.box-header -->
        <div class="box-body">
            <div class="form-group">
                @Html.LabelFor(m => m.PropertyInfo.PropertyName)
                @Html.TextBoxFor(m => m.PropertyInfo.PropertyName, new { @class = "form-control" })
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.PropertyInfo.Address1)
                @Html.TextBoxFor(m => m.PropertyInfo.Address1, new { @class = "form-control" })
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.PropertyInfo.Address2)
                @Html.TextBoxFor(m => m.PropertyInfo.Address2, new { @class = "form-control" })
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.PropertyInfo.City)
                @Html.TextBoxFor(m => m.PropertyInfo.City, new { @class = "form-control" })
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.PropertyInfo.State)
                @Html.TextBoxFor(m => m.PropertyInfo.State, new { @class = "form-control" })
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.PropertyInfo.ZIP)
                @Html.TextBoxFor(m => m.PropertyInfo.ZIP, new { @class = "form-control" })
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.PropertyInfo.Country)
                @Html.TextBoxFor(m => m.PropertyInfo.Country, new { @class = "form-control" })
            </div>
        </div><!-- /.box-body -->
    </div><!-- /.box -->

</div><!--/.col (left) -->

<div class="col-md-3">
</div>