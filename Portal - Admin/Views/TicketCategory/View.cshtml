@model ResComm.Web.Models.TicketCategory
@{
    ViewBag.Title = "Ticket Category Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Title { Details }
@section ContentHeader{

    <div class="pull-right">
        <a href="@Url.Action("List", "TicketCategory")" class="btn btn-primary inline"><i class="fa fa-arrow-left"></i> Back</a>
        <a href="@Url.Action("Edit", "TicketCategory", new { Id = Model.TicketCategoryId })" class="btn btn-primary inline"><i class="fa fa-edit"></i> Edit</a>
    </div>
}

<div class="row">
    <div class="col-md-12">
        <div class="box box-danger">
            <div class="box-header">
                <h3 class="box-title">TicketCategory Information</h3>
            </div><!-- /.box-header -->
            <div class="box-body">
                <div class="form-group">
                    @Html.LabelFor(m => m.Name)
                    <div>
                        @Model.Name
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.Description)
                    <div>
                        @Model.Description
                    </div>
                </div>
            </div><!-- /.box-body -->
        </div><!-- /.box -->

    </div><!--/.col (left) -->
</div><!-- /.row -->