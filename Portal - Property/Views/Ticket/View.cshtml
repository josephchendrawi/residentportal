@model ResComm.Web.Models.Ticket
@{
    ViewBag.Title = "Complain & Defect Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Title { Details }
@section ContentHeader{
    <script>
            $(function () {
                $('#main_title').html("Complain & Defect");
            });
    </script>

    <div class="pull-right">
        <a href="@Url.Action("List", "Ticket")" class="btn btn-primary inline"><i class="fa fa-arrow-left"></i> Back</a>
    </div>
}

<div class="row">
    <div class="col-md-12">
        <div class="box box-danger">
            <div class="box-header">
                <h3 class="box-title">Ticket Information</h3>
            </div><!-- /.box-header -->
            <div class="box-body">
                <div class="form-group">
                    @Html.LabelFor(m => m.Name)
                    <div>
                        @Model.Name
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.Requester)
                    <div>
                        @Model.Requester
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.Created)
                    <div>
                        @Model.Created
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.CategoryId)
                    <div>
                        @Model.CategoryName
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.Priority)
                    <div>
                        @Model.Priority
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.Status)
                    <div>
                        @Model.Status
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.Description)
                    <div>
                        @Model.Description
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.Type)
                    <div>
                        @Model.Type
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.EstStartDate)
                    <div>
                        @Model.EstStartDateText
                    </div>
                </div>
                <div class="form-group">
                    @Html.LabelFor(m => m.EstEndDate)
                    <div>
                        @Model.EstEndDateText
                    </div>
                </div>
            </div><!-- /.box-body -->
        </div><!-- /.box -->

    </div><!--/.col (left) -->
</div><!-- /.row -->